ALGORITHM comm
VAR
    A : ARRAY_OF INTEGER[] ;=[12, 13, 6, 10];
    
    B : ARRAY_OF INTEGER[] ;=[13, 10, 16, 15];

BEGIN
    FOR i FROM 1 TO A.length STEP 1  DO

           FOR c FROM 1 TO B.length STEP 1  DO
                IF (A[i]=B[c]) THEN
                    sum := sum + A[i]*2
                END_IF
                      
                 
            END_FOR




            END_FOR   
END